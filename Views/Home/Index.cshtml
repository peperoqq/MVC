

@using PagedList
@using PagedList.Mvc
@model IPagedList<prjFinal.Models.TableFinalExams1091746>
@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>
<table class="table table-striped">
    <tr>
        <th>訂單編號</th>
        <th>訂單日期</th>
        <th>送貨日期</th>
        <th>收貨人</th>
        <th>送貨地址</th>
        <th>運費</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.訂單號碼)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.訂單日期)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.送貨日期)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.收貨人)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.送貨地址)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.運費)
            </td>
        </tr>
    }
</table>


@Html.PagedListPager(Model, page => Url.Action("Index", new { page }))